<?xml version="1.0" encoding="utf-8"?>
<cn.reee.reeeplayer.view.AdapterView.ZpRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VideoViewPlayTsActivity">

    <VideoView
        android:id="@+id/video_view"
        android:layout_width="160dp"
        android:layout_height="90dp"

        android:layout_centerHorizontal="true" />

    <VideoView
        android:id="@+id/video_view_second"
        android:layout_width="160dp"

        android:layout_height="90dp"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/bt_play"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:text="PLAY" />

</cn.reee.reeeplayer.view.AdapterView.ZpRelativeLayout>